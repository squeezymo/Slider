<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Slider">
        <attr name="sld_type" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
            <enum name="planar_locked" value="2" />
            <enum name="planar_unlocked" value="3" />
        </attr>
        <attr name="sld_backgroundStill" format="reference" />
        <attr name="sld_backgroundDragged" format="reference" />
        <attr name="sld_segmentsX" format="integer" />
        <attr name="sld_segmentsY" format="integer" />
        <attr name="sld_alphaFromX" format="float" />
        <attr name="sld_alphaToX" format="float" />
        <attr name="sld_alphaFromY" format="float" />
        <attr name="sld_alphaToY" format="float" />
    </declare-styleable>
</resources>